{
  "schemaVersion": 1,
  "id": "wyoming",
  "version": "${version}",

  "name": "Wyoming",
  "description": "Wynncraft utility mod",
  "authors": [
    "Lexi (l3x1)"
  ],
  "contact": {
    "homepage": "https://github.com/13x1/wyoming",
    "sources": "https://github.com/13x1/wyoming",
    "issues": "https://github.com/13x1/wyoming/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/wyoming/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "cx.lexi.wyoming.Wyoming"
      }
    ]
  },
  "mixins": [
    "wyoming.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.12",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.0+kotlin.1.8.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}
